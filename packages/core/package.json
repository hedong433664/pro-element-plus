{
  "name": "@coderhd/pro-element-plus",
  "version": "1.1.2",
  "description": "Components library built with Vue3 + TypeScript",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "build": "run-s remove-dist build-only move-style",
    "build-only": "run-p build-es build-umd",
    "build-es": "vite build --config vite.es.config.ts",
    "build-umd": "vite build --config vite.umd.config.ts",
    "move-style": "test -f 'dist/es/index.css' && mv 'dist/es/index.css' 'dist/index.css'",
    "remove-dist": "rimraf dist"
  },
  "keywords": [
    "pro-element-plus",
    "element-plus",
    "components",
    "pro-components",
    "vue3",
    "typescript",
    "ui-library"
  ],
  "author": "hedong433664",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hedong433664/pro-element-plus.git"
  },
  "bugs": {
    "url": "https://github.com/hedong433664/pro-element-plus/issues"
  },
  "homepage": "https://hedong433664.github.io/pro-element-plus/",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "element-plus": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": false
    }
  },
  "dependencies": {
    "@pro-element-plus/components": "workspace:*"
  },
  "devDependencies": {},
  "packageManager": "pnpm@10.6.5"
}
